	<FactionColonies.BuildingFCDef>
		<defName>plagueCamp</defName> <!--Name of Def -->
		<label>Plague Camp</label> <!--Lable of Def -->
		<desc>A containment camp to keep the sick and the dying. -3 worker max. -2 worker overmax. +0.4 research and +0.4 medicine production.</desc> <!-- Description of trait -->
		<cost>500</cost>
		<techLevel>Medieval</techLevel>
		<constructionDuration>120000</constructionDuration> <!-- Ticks it takes until construction is finished -->
		<traits> <!-- Put the traits they provide here -->
			<li>plagueCamp</li>
		</traits>
		<applicableBiomes> <!-- If the building requires it to be in a biome, put it here-->
		
		</applicableBiomes>
		<upkeep>0</upkeep> <!-- cost per week for the building -->
		<iconPath>GUI/Buildings/PlagueCamps</iconPath>
	</FactionColonies.BuildingFCDef>
	
	<FactionColonies.FCTraitEffectDef>
		<defName>plagueCamp</defName> <!--Name of Def -->
		<label>Plague Camp</label> <!--Lable of Def -->
		<desc></desc>
		<workerBaseMax>-3</workerBaseMax>
		<workerBaseOverMax>-2</workerBaseOverMax>
		<productionBaseResearch>.4</productionBaseResearch>
		<productionBaseMedicine>.4</productionBaseMedicine>
	</FactionColonies.FCTraitEffectDef>




